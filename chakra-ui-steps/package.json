{
  "version": "2.1.0",
  "license": "MIT",
  "name": "chakra-ui-steps",
  "author": "Jean Verster",
  "keywords": [
    "steps",
    "stepper",
    "chakra",
    "navigation",
    "multi"
  ],
  "main": "./dist/chakra-ui-steps.umd.cjs",
  "module": "./dist/chakra-ui-steps.mjs",
  "exports": {
    ".": {
      "import": "./dist/chakra-ui-steps.mjs",
      "require": "./dist/chakra-ui-steps.umd.cjs"
    }
  },
  "types": "dist/index.d.ts",
  "description": "Steps component designed to work seamlessly with Chakra UI",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jeanverster/chakra-ui-steps"
  },
  "scripts": {
    "build-storybook": "build-storybook",
    "build": "tsc --noEmit && vite build",
    "coverage": "vitest run --coverage",
    "dev": "vite build --watch",
    "prepare": "yarn build",
    "prestorybook": "rimraf node_modules/.cache/storybook",
    "preview": "vite preview",
    "storybook": "start-storybook -p 6006",
    "test": "vitest"
  },
  "peerDependencies": {
    "@chakra-ui/react": "^2.0.0",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "framer-motion": "^7.0.0",
    "react": "^18.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "homepage": "https://github.com/jeanverster/chakra-ui-steps",
  "devDependencies": {
    "@chakra-ui/transition": "^2.0.2",
    "@rollup/plugin-typescript": "^8.3.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.2",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addons": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@testing-library/react": "^13.3.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.0.7",
    "husky": "^6.0.0",
    "jsdom": "^19.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-icons": "^4.2.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.2",
    "semantic-release-monorepo": "^7.0.5",
    "storybook-addon-performance": "^0.16.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "vite": "^4.2.1",
    "vitest": "^0.29.7"
  },
  "release": {
    "branches": [
      "develop"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/git"
    ],
    "extends": "semantic-release-monorepo"
  }
}
